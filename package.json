{
  "name": "lybc-web",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://www.hklybc.org/",
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "lodash": "^4.17.15",
    "moment": "2.24.0",
    "node-sass": "^4.13.1",
    "nouislider": "14.0.2",
    "react": "16.8.6",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-bootstrap-switch": "15.5.3",
    "react-datetime": "2.16.3",
    "react-dom": "16.8.6",
    "react-redux": "^7.1.0-rc.1",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "react-scripts": "^3.3.1",
    "reactstrap": "8.0.1",
    "redux-saga": "^1.1.1",
    "reselect": "^4.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "node-sass src/assets/scss/now-ui-kit.scss src/assets/css/now-ui-kit.css",
    "minify-sass": "node-sass src/assets/scss/now-ui-kit.scss src/assets/css/now-ui-kit.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/now-ui-kit.scss src/assets/css/now-ui-kit.css --source-map true",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "optionalDependencies": {
    "typescript": "3.5.3"
  },
  "devDependencies": {
    "gh-pages": "^2.1.1"
  }
}
